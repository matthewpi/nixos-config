		"experimentalDecorators": true,
		"module": "ES2022",
		"target": "ES2022",
		"moduleResolution": "Bundler",

		"paths": {
			"astal": ["/nix/store/36kxv4nh9kcsjcjdjv3xsvqnpk53xja4-astal-gjs/share/astal/gjs"],
			"astal/*": ["/nix/store/36kxv4nh9kcsjcjdjv3xsvqnpk53xja4-astal-gjs/share/astal/gjs/*"]
